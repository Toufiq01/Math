class Solution {
    public int[] runningSum(int[] nums) {
        // int f1,f2,result;
        int [] output = new int[nums.length];
        // output[0] =nums[0];
        int result = 0;
        for (int i=0; i < nums.length; i++){
            result += nums[i];
            output[i] = result;
        }
        return output;
    }
}
